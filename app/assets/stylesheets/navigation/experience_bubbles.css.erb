.experience-bubble {
  width: 100%;
  height: 100px;
  border-radius: 100%;
}

.green {
  background-color: green;
}

.red {
  background-color: red;
}

nav {
  width: 100px;
  height: 100vh;
  overflow: auto;
  position: fixed;
  background-color: transparent;
  overflow-x: hidden;
  overflow-y: auto;
  z-index: 1;
}

::-webkit-scrollbar {
    display: none;
}
