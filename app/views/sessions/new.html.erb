
  <div id="user-login-box">
    <h1 id="user-login-title">User Login</h1>
    <h4>New user? Register <a href="#">here</a></h4>
    <%=form_for sessions_path do |f| %>
		<%=f.text_field :username, class: "form-input" %>
		<%=f.text_field :password, class: "form-input"%>
		<%= f.submit "Login", class: 'form-submit' %>
	<% end %>
  </div>



